<Window x:Class="Date_Check_Tool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Date_Check_Tool"
        mc:Ignorable="d"
        Title="Date Check" Height="240" Width="240" Closing="Window_Closing" Background="#FF3F51B5" ResizeMode="NoResize" Icon="checkbox.png">
    <Grid Height="202" Width="234">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="browseButton" Content="Browse" Margin="78,10,78,0" VerticalAlignment="Top" Click="browseButton_Click" Height="78" Foreground="White"/>
        <Button x:Name="goButton" Content="Go" Margin="75,170,75,10" Click="goButton_Click" Foreground="White" Height="22" Width="84"/>
        <ComboBox x:Name="tableComboBox" Margin="39,139,39,0" VerticalAlignment="Top" Style="{StaticResource {x:Static ToolBar.ComboBoxStyleKey}}" Background="White" BorderBrush="White" Foreground="Black" HorizontalAlignment="Stretch" Height="20"/>
        <Label x:Name="nameLabel" Content="Open a link file." Margin="0,98,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Center" Foreground="White" Background="#FF394AA8" Height="30"/>

    </Grid>
</Window>
